        "// Constants\n",
        "const API_KEY  = \"";\n",
        "const MODEL_TYPE = \"gpt-3.5-turbo\"; //chatGPT model\n",
        "\n",
        "function onOpen() {\n",
        " DocumentApp.getUi().createMenu(\"GPT3 MAGIC\")\n",
        " .addItem(\"Generate chapter\", \"generatchapter\" )\n",
